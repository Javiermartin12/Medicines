{
  "compilerOptions": {
    "target": "ES6",
    "jsx": "react-native",
    "module": "commonjs",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "*"
      ]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
